clear all, close all, clc
for i=1:104
    try
        img=strcat('lunares/',sprintf('%03d',i),'.jpg');
        a=imread(img);[fil,col,cap]=size(a);
        cform=makecform('srgb2lab');lab=applycform(a,cform);
        b=lab(:,:,3);
        b=double(b);b=b/max(b(:))*255;b=uint8(b);
        b=[b,b,b];b=reshape()
        figure(1);imshow(b);
    end
end